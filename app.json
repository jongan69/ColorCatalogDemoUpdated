{
  "expo": {
    "name": "ColorCatalog",
    "description": "Catalog your colors. Save your favorite web colors and view the details",
    "slug": "ColorCatalog",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/cc-icon.png",
    "splash": {
      "image": "./assets/cc-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/cc-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/cc-icon.png"
    }
  }
}
